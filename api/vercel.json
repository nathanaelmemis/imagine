{
    "version": 2,
    "functions": {
      "api/file.js": {
        "includeFiles": "greet.cjs"
      }
    },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }